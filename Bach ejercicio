@echo off
:menu
cls
echo.       menu
echo.       
echo.       a.- opcion -1
echo.       b.- salir
echo.       
set /P Opc=Su opcion es:

IF "%Opc%"    ==  "a" goto Op1
IF "%Opc%"    ==  "d" goto salir

:Op1
echo Has elegido Opcion - 1
::Aqui van la líneas de comando de tu opción
color 02
pause
goto

:salir
@cls&exit
